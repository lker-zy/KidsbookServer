    <mkdir dir="${artifact.temp.output.kidsbook-common:jar}"/>
    <jar destfile="${temp.jar.path.kidsbook-common.jar}" duplicate="preserve" filesetmanifest="mergewithoutmain">
      <zipfileset dir="${kidsbook-common.output.dir}"/>
    </jar>
    <copy file="${temp.jar.path.kidsbook-common.jar}" tofile="${artifact.temp.output.kidsbook-common:jar}/kidsbook-common.jar"/>
  </target>
  
