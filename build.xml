    <property name="artifacts.temp.dir" value="${basedir}/__artifacts_temp"/>
    <property name="artifact.output.kidsbook-common:jar" value="${basedir}/mis/web/WEB-INF/lib"/>
    <property name="artifact.output.mis:war_exploded" value="${basedir}/out/artifacts/mis_war_exploded"/>
    <property name="artifact.output.user:war_exploded" value="${basedir}/out/artifacts/user_war_exploded"/>
    <property name="artifact.output.web" value="${basedir}/out/artifacts/web"/>
    <property name="artifact.output.web:war_exploded" value="${basedir}/out/artifacts/web_war_exploded"/>
    <mkdir dir="${artifacts.temp.dir}"/>
    <property name="temp.jar.path.kidsbook-common.jar" value="${artifacts.temp.dir}/kidsbook-common.jar"/>
  </target>
  
  <target name="clean.artifact.mis:war_exploded" description="clean mis:war exploded artifact output">
    <delete dir="${artifact.output.mis:war_exploded}"/>
  </target>
  
  <target name="clean.artifact.user:war_exploded" description="clean user:war exploded artifact output">
    <delete dir="${artifact.output.user:war_exploded}"/>
  </target>
  
  <target name="clean.artifact.web" description="clean web artifact output">
    <delete dir="${artifact.output.web}"/>
  </target>
  
  <target name="clean.artifact.web:war_exploded" description="clean web:war exploded artifact output">
    <delete dir="${artifact.output.web:war_exploded}"/>
  </target>
  
